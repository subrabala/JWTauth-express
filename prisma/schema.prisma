generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id        String   @id @default(uuid())
    firstname String
    lastname  String
    password  String
    phone     String
    privilege Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Movies {
    id        String   @id @default(uuid())
    moviename String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
